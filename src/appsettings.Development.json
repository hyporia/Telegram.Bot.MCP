{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo:File": {
      "Name": "File",
      "Args": {
        "path": "./logs/log-.log",
        "rollingInterval": "Day"
      }
    }
  }
}
